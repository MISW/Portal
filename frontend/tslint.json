{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint:react",
        "tslint-react-hooks"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [true, "always"],
        "quotemark": [true, "single", "jsx-double"],
        "react-hooks-nesting": "error",
        "no-floating-promises": true,
        "array-type": false
    },
    "rulesDirectory": []
}
